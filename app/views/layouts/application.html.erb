<!DOCTYPE html>
<html>
<head>
    <title><%= generate_title yield :title %></title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= stylesheet_link_tag    'http://fonts.useso.com/icon?family=Material+Icons' %>
    <%= stylesheet_link_tag    'http://fonts.useso.com/css?family=Abel', ref: 'stylesheet', type: 'text/css' %>
    <%= javascript_include_tag 'application' %>
    <%= yield :head %>
    <%= csrf_meta_tags %>
</head>
<body id="body">
    <%= render 'shared/navbar' %>

    <%= yield %>

    <%= render 'shared/footer' %>
</body>
</html>
